<div class='container'>
  <h1>Agrega tu evento</h1>
  <p>Completa el siguiente formulario.</p>
  <%= simple_form_for [@car, @app] do |f| %>
    <%= f.input :price %>
    <%= f.input :pickup_point %>
    <%= f.input :drop_point %>
    <%= f.input :pickup_datetime %>
    <%= f.input :country, as: :string %>
    <%= f.input :city %>
    <%= f.input :comune %>
    <%= f.input :confirmation_status, as: :hidden, input_html: { value: false } %>

    <%= f.submit %>
  <% end %>
</div>
