<div class="container mt3">
  <h3> Mis autos:</h3>
  <h5> Elegi un auto, para realizar el viaje.</h5>
    <% @misautos.each do |car| %>
      <div class= 'card-product my-4 d-flex '>
        <div class="container">
            <div class="card-product-infos">
              <%= image_tag("Traffic.webp") %>
              <h6>Model: <%= car.model %></h6>
              <h6>Brand: <%= car.brand %>
              <h6>Color: <%= car.color %></h6>
              <h6>Transmission: <%= car.transmission %></h6>
              <h6>Seguro para terceros: <%= car.insurance_other_drivers %></h6>
              <div class="d-flex justify-content-end w-100 align-items mb-4 h-100 align-items-end">
                  <div class="me-4">
                    <%= link_to "Seleccionar",   new_car_application_path(car), class: "btn btn-primary" %>
                  </div>
              </div>
            </div>
        </div>
      </div>
    <% end %>
</div>
